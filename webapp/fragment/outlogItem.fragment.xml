<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.ui.table"
    xmlns:plugins="sap.ui.table.plugins"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:u="sap.ui.unified"
    xmlns:c="sap.ui.core"
    xmlns:m="sap.m"
>
    <Table
        id="idDeviceDataSetTable"
        rows="{ path: 'OutLogModel>/DeviceDataSet'}"
        rowMode="Auto"
        enableBusyIndicator="true"
        fixedColumnCount="1"
        ariaLabelledBy="title"
    >
        <extension>
            <m:OverflowToolbar style="Clear">
                <m:Title
                    id="title"
                    text="Products"
                />
                <m:ToolbarSpacer />
                <m:Button
                    visible="true"
                    icon="sap-icon://delete"
                    tooltip="{i18n>DelLineItem}"
                    press="onHandleDelete"
                />
            </m:OverflowToolbar>
        </extension>
        <rowSettingsTemplate>
            <RowSettings
                highlight="{
                                path: 'OutLogModel>Type',
                                formatter :'.formatter.fnMessageType'
                            }"
                highlightText="{OutLogModel>Message}"
            />
        </rowSettingsTemplate>
        <columns>
            <Column
                sortProperty="Name"
                filterProperty="Name"
                autoResizable="true"
                width="11rem"
                hAlign="Right"
            >
                <m:Label text="{/#DeviceData/SerialNumber/@sap:label}" />
                <template>
                    <m:Text
                        text="{OutLogModel>SerialNumber}"
                        wrapping="false"
                    />
                </template>
            </Column>
            <Column
                sortProperty="ProductID"
                filterProperty="ProductID"
                autoResizable="true"
                width="6rem"
                hAlign="Right"
            >
                <m:Label text="{/#DeviceData/Plant/@sap:label}" />
                <template>
                    <m:Text
                        text="{OutLogModel>Plant}"
                        wrapping="false"
                    />
                </template>
            </Column>
            <Column
                sortProperty="Category"
                filterProperty="Category"
                autoResizable="true"
                width="11rem"
                hAlign="Left"
            >
                <m:Label text="{/#DeviceData/StorageLocation/@sap:label}" />
                <template>
                    <m:Text
                        text="{OutLogModel>StorageLocation}"
                        wrapping="false"
                    />
                </template>
            </Column>
            <Column
                hAlign="Left"
                width="9rem"
            >
                <m:Label text="{/#DeviceData/Recipient/@sap:label}" />
                <template>
                    <m:Text
                        text="{OutLogModel>Recipient}"
                        wrapping="false"
                    />
                </template>
            </Column>
            <Column
                hAlign="End"
                width="9rem"
            >
                <m:Label text="{/#DeviceData/DocumentDate/@sap:label}" />
                <template>
                    <m:Text
                        text="{
						'path':'OutLogModel>DocumentDate',
						'type':'sap.ui.model.type.Date',
						'formatOptions': {
							'pattern': 'MM-dd-yyyy'
						}}"
                    />
                </template>
            </Column>
            <Column
                hAlign="End"
                autoResizable="true"
                width="9rem"
            >
                <m:Label text="{/#DeviceData/PostingDate/@sap:label}" />
                <template>
                    <m:Text
                        text="{
						'path':'OutLogModel>PostingDate',
						'type':'sap.ui.model.type.Date',
						'formatOptions': {
							'pattern': 'MM-dd-yyyy'
						}}"
                        wrapping="false"
                    />
                </template>
            </Column>
            <Column
                hAlign="End"
                autoResizable="true"
                width="9rem"
            >
                <m:Label text="{/#DeviceData/MaterialNumber/@sap:label}" />
                <template>
                    <m:Text
                        text="{OutLogModel>MaterialNumber}"
                        wrapping="false"
                    />
                </template>
            </Column>
            <Column
                hAlign="End"
                autoResizable="true"
                width="9rem"
            >
                <m:Label text="{/#DeviceData/MaterialDescription/@sap:label}" />
                <template>
                    <m:Text
                        text="{OutLogModel>MaterialDescription}"
                        wrapping="false"
                    />
                </template>
            </Column>
