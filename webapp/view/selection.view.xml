<mvc:View
    controllerName="com.lh.sapui5.zcrtinlogoutlog.controller.selection"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page showHeader="false">
        <content>
            <GenericTile
                id="idOutlog"
                class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                header="{i18n>lhoutlogTiletitleTxt}"
                subheader="{i18n>lhInlogOutlogtileSubHeading}"
                press="onPressOutlog"
                frameType="OneByOne"
            >
                <TileContent>
                    <ImageContent src="sap-icon://inspect" />
                </TileContent>
            </GenericTile>
            <GenericTile
                id="idInlog"
                class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                header="{i18n>lhinlogTiletitleTxt}"
                subheader="{i18n>lhInlogOutlogtileSubHeading}"
                press="onPressInlog"
                frameType="OneByOne"
            >
            <TileContent>
                    <ImageContent src="sap-icon://add-equipment" />
                </TileContent>
            </GenericTile>
        </content>
    </Page>
</mvc:View>
