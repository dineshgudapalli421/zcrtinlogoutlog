<mvc:View
    controllerName="com.lh.sapui5.zcrtinlogoutlog.controller.outlog"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    xmlns:f="sap.f"
    xmlns="sap.m"
>
    <f:DynamicPage headerExpanded="{/headerExpanded}" showFooter="true" fitContent="true">
    <f:title>
				<f:DynamicPageTitle>
					<f:heading>
						<Title text="{i18n>createOutlogTitle}"/>
					</f:heading>
				</f:DynamicPageTitle>
			</f:title>
        <f:header>
            <core:Fragment
                fragmentName="com.lh.sapui5.zcrtinlogoutlog.fragment.outlogHeader"
                type="XML"
            />
        </f:header>
        <f:content>
        <core:Fragment
                fragmentName="com.lh.sapui5.zcrtinlogoutlog.fragment.outlogItem"
                type="XML"
            />
        </f:content>
        <f:footer>
				<OverflowToolbar>
					<ToolbarSpacer/>
					<Button id="idAcceptBtn" type="Accept" text="{i18n>lhoutlogPostBtnTxt}" press="onPressPostButton"/>
					<Button id="idCancelBtn" type="Reject" text="{i18n>lhoutlogCancelBtnTxt}" press="onPressCancelButton"/>
				</OverflowToolbar>
			</f:footer>
    </f:DynamicPage>
</mvc:View>
